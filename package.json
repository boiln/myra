{
    "name": "myra",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "tauri": "tauri",
        "fmt:js": "prettier --config .prettierrc.json --ignore-path .prettierignore --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "fmt:check": "prettier --config .prettierrc.json --ignore-path .prettierignore --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "lint": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
        "fmt:rust": "cd src-tauri && cargo fmt --all",
        "rust:check": "cd src-tauri && cargo clippy --all-features -- -D warnings",
        "format": "powershell -ExecutionPolicy Bypass -File .\\format.ps1",
        "format-js": "pnpm run fmt:js && pnpm run lint",
        "format-rust": "cd src-tauri && cargo fmt --all && cargo clippy --fix --allow-dirty --allow-no-vcs",
        "check": "pnpm fmt:check && eslint \"**/*.{ts,tsx,js,jsx}\" && pnpm run rust:check"
    },
    "dependencies": {
        "@radix-ui/react-alert-dialog": "^1.1.6",
        "@radix-ui/react-checkbox": "^1.1.4",
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-dropdown-menu": "^2.1.6",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-radio-group": "^1.2.3",
        "@radix-ui/react-select": "^2.1.6",
        "@radix-ui/react-separator": "^1.1.2",
        "@radix-ui/react-slider": "^1.2.3",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-switch": "^1.1.3",
        "@radix-ui/react-tabs": "^1.1.3",
        "@radix-ui/react-toast": "^1.1.5",
        "@radix-ui/react-tooltip": "^1.1.8",
        "@tauri-apps/api": "^2.9.1",
        "@tauri-apps/plugin-fs": "^2.4.4",
        "@tauri-apps/plugin-opener": "^2.5.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "framer-motion": "^12.5.0",
        "lucide-react": "^0.562.0",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "react-router-dom": "^7.3.0",
        "shadcn": "3.6.2",
        "sonner": "^2.0.1",
        "tailwind-merge": "^3.0.2",
        "tailwindcss-animate": "^1.0.7",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.22.0",
        "@tauri-apps/cli": "^2.3.1",
        "@types/node": "^25.0.3",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8.26.0",
        "@typescript-eslint/parser": "^8.26.0",
        "@vitejs/plugin-react": "^5.1.2",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "tailwindcss": "3.4.1",
        "typescript": "^5.8.2",
        "vite": "^7.3.0"
    }
}
